{
    "name": "iz5clj/laravel-multi-theme",
    "description": "A Laravel package for multi-theme support with dynamic view path switching",
    "keywords": ["laravel", "theme", "views", "multi-theme"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Michel",
            "email": "ms@auxe.net"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/view": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Iz5clj\\LaravelMultiTheme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iz5clj\\LaravelMultiTheme\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iz5clj\\LaravelMultiTheme\\ThemeServiceProvider"
            ],
            "aliases": {
                "Theme": "Iz5clj\\LaravelMultiTheme\\Facades\\Theme"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
